    "#selector de columnas a desmantelar\n",
    "def SSRC(layout):\n",
    "    best_ev = 0; s_o=-1\n",
    "    for i in range(len(layout.stacks)):\n",
    "        if layout.is_sorted_stack(i): continue\n",
    "        prom = sum(layout.stacks[i]) / len(layout.stacks[i]) \n",
    "        ev = 10000 - 100*len(layout.stacks[i]) - prom\n",
    "\n",
    "        if ev > best_ev:\n",
    "            best_ev = ev\n",
    "            s_o = i\n",
    "    return s_o"
